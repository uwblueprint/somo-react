{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "jsx-a11y/no-static-element-interactions": 0,
    "react/forbid-prop-types": 0,
    "react/prefer-stateless-function": 0,
    "import/extensions": 0,
    "no-undef": 0,
    "no-console": 0,
    "no-plusplus": 0,
    "no-continue": 0
  }
}
